{
  "name": "MediQ-Frontend",
  "slug": "MediQ-Frontend",
  "version": "1.0.0",
  "orientation": "portrait",
  "icon": "./assets/icon.png",
  "scheme": "mediqfrontend",
  "userInterfaceStyle": "automatic",
  "newArchEnabled": true,
  "plugins": [
    "expo-router",
    [
      "expo-splash-screen",
      {
        "image": "./assets/splash-icon.png",
        "imageWidth": 200,
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    ],
    "expo-font",
    "expo-secure-store"
  ],
  "sdkVersion": "53.0.0",
  "platforms": ["ios", "android", "web"],
  "ios": {
    "supportsTablet": true
  },
  "android": {
    "edgeToEdgeEnabled": true,
    "package": "com.jsjsjsjsjsjsjs.MediQFrontend",
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#ffffff"
    }
  },
  "web": {
    "bundler": "metro",
    "output": "static",
    "favicon": "./assets/favicon.png"
  },
  "experiments": {
    "typedRoutes": true
  },
  "extra": {
    "router": {},
    "eas": {
      "projectId": "e5fa72e7-5362-4f64-b636-74aba52f40fb"
    }
  },
  "androidStatusBar": {
    "backgroundColor": "#ffffff"
  }
}
